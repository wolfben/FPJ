@{
    ViewBag.Title = "首页";
    var articleList = ViewBag.articleList as List<Article>;
}
@model List<User>

<table class="table">
    <tr>
        <th>ID</th>
        <th>用户名</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserId</td>
            <td>@item.UserName</td>
            <td><a href="/home/detail/@item.UserId">详情</a></td>
        </tr>  
    }
</table>

<table class="table">
    <tr>
        <th>ID</th>
        <th>标题</th>
        <th>内容</th>
    </tr>
    @foreach (var item in articleList)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.Content</td>
        </tr>
    }
</table>
<p>@ViewBag.name</p>